  "scripts": {
    "build-modernizr": "node ./config/modernizr.js",
    "start": "npm run build-modernizr && node scripts/start.js",
    "build": "npm run build-modernizr && node scripts/build.js && cp -i sitemap.xml build/",
    "clear:cloudfront:test": "aws configure set preview.cloudfront true && aws cloudfront create-invalidation --distribution-id E1WATHXVA3QL8F --paths '/*'",
    "clear:cloudfront:test2": "aws configure set preview.cloudfront true && aws cloudfront create-invalidation --distribution-id E3UT00JNK39KXY --paths '/*'",
    "clear:cloudfront:prod": "aws configure set preview.cloudfront true && aws cloudfront create-invalidation --distribution-id EOXV33AAQ28HB --paths '/*'",
    "deploy:test:env": "better-npm-run deploy:test",
    "deploy:prod:env": "better-npm-run deploy:prod",
    "version_number:test": "curl -X post http://api.test.autolife.ca/get_version/",
    "version_number:prod": "curl -X post http://api.autolife.ca/get_version/",
    "test": "node scripts/test.js",
    "deploy:test": "npm run version_number:test && npm run deploy:test:env && aws s3 rm s3://test.autolife.ca --recursive && aws s3 sync build/ s3://test.autolife.ca --profile dev_admin && npm run clear:cloudfront:test",
    "deploy:test2": "npm run version_number:test && npm run deploy:test:env && aws s3 rm s3://test2.autolife.ca --recursive && aws s3 sync build/ s3://test2.autolife.ca --profile dev_admin && npm run clear:cloudfront:test2",
    "deploy:prod": "npm run version_number:prod && REACT_APP_ENV=production GENERATE_SOURCEMAP=false npm run build && aws s3 rm s3://autolife.ca --recursive && aws s3 sync build/ s3://autolife.ca --profile prod_admin && npm run clear:cloudfront:prod",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "betterScripts": {
    "deploy:test": {
      "command": "npm run build",
      "env": {
        "REACT_APP_ENV": "test"
      }
    },
    "deploy:prod": {
      "command": "npm run build",
      "env": {
        "REACT_APP_ENV": "production"
      }
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "env": {
      "production": {
        "plugins": [
          [
            "transform-remove-console",
            {
              "exclude": [
                "error"
              ]
            }
          ]
        ]
      }
    }
  },
  "devDependencies": {
    "@babel/register": "^7.0.0",
    "@storybook/addon-actions": "^5.1.10",
    "@storybook/addon-knobs": "^5.1.10",
    "@storybook/addon-links": "^5.1.10",
    "@storybook/addons": "^5.1.10",
    "@storybook/react": "^5.1.10",
    "babel-plugin-transform-remove-console": "^6.9.4"
  }
}
